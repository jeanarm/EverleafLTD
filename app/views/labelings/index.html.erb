<p id="notice"><%= notice %></p>

<h1>Labelings</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Task</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labelings.each do |labeling| %>
      <tr>
        <td><%= labeling.label %></td>
        <td><%= labeling.task %></td>
        <td><%= link_to 'Show', labeling %></td>
        <td><%= link_to 'Edit', edit_labeling_path(labeling) %></td>
        <td><%= link_to 'Destroy', labeling, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Labeling', new_labeling_path %>
